{
  "name": "apartment-management-system-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "rm -rf dist && mkdir dist && babel src -s -d dist",
    "start": "export NODE_ENV=production && npm run build && node dist/index.js",
    "start:dev": "nodemon --exec babel-node src/index.js",
    "db:create": "sequelize db:create",
    "db:reset": "npm run db:migrate:reset && npm run db:migrate && npm run db:seed",
    "db:drop": "sequelize db:drop",
    "db:migrate": "sequelize db:migrate",
    "db:migrate:reset": "sequelize db:migrate:undo:all",
    "db:seed": "sequelize db:seed:all",
    "db:migrate:seed": "sequelize db:migrate && npm run db:seed",
    "pm2:start": "pm2 start app.json",
    "pm2:deploy": "npm run build && npm run db:reset &&npm run pm2:start",
    "pm2:stop": "pm2 stop app.json",
    "pm2:destroy": "pm2 delete app.json",
    "pm2:logs": "pm2 logs api"
  },
  "devDependencies": {
